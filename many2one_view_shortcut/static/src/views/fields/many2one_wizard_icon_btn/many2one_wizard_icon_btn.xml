<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve" owl="1">
    <t t-inherit="web.Many2OneField" t-inherit-mode="extension">
        <xpath expr="//t[@t-if='props.readonly']/t[@t-else='']/a/span" position="after">
            <t t-if="hasExternalButton">
                    <button
                        type="button"
                        class="btn btn-link text-action oi o_external_button oi-view"
                        tabindex="-1"
                        draggable="false"
                        aria-label="Open Wizard"
                        data-tooltip="Open Wizard"
                        t-on-click="onExternalWizardBtnClick"
                    />
                </t>
                <t t-if="hasExternalButton">
                    <button
                        type="button"
                        class="btn btn-link text-action oi o_external_button oi-launch"
                        tabindex="-1"
                        draggable="false"
                        aria-label="Open Link"
                        data-tooltip="Open Link"
                        t-on-click="onExternalNewTabBtnClick"
                    />
                </t>
        </xpath>
        <xpath expr="//div[@class='o_field_many2one_selection']" position="inside">
                <t t-if="hasExternalButton">
                    <button
                        type="button"
                        class="btn btn-link text-action oi o_external_button oi-view"
                        tabindex="-1"
                        draggable="false"
                        aria-label="Open Wizard"
                        data-tooltip="Open Wizard"
                        t-on-click="onExternalWizardBtnClick"
                    />
                </t>
                <t t-if="hasExternalButton">
                    <button
                        type="button"
                        class="btn btn-link text-action oi o_external_button oi-launch"
                        tabindex="-1"
                        draggable="false"
                        aria-label="Open Link"
                        data-tooltip="Open Link"
                        t-on-click="onExternalNewTabBtnClick"
                    />
                </t>
        </xpath>
    </t>
</templates>